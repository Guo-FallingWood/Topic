<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry location="web/WEB-INF/lib/mysql-connector-java-5.1.35.jar" />
	<context id="jdbc">
		<commentGenerator>
			<property name="suppressAllComments" value="true"/>
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						userId="root"
						password="123456"
						connectionURL="jdbc:mysql://localhost:3306/topic">
		</jdbcConnection>

		<javaModelGenerator targetPackage="com.sang.topic.model" targetProject="src">
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.sang.topic.data" targetProject="src">
		</sqlMapGenerator>
		
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.sang.topic.data" targetProject="src">
		</javaClientGenerator>

		<table tableName="topic_user" domainObjectName="User"  enableCountByExample="false" enableDeleteByExample="false"
			 enableSelectByExample="true" enableUpdateByExample="false"  selectByExampleQueryId="false">
		</table>
		<table tableName="topic_comments" domainObjectName="Comments"  enableCountByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="true" enableUpdateByExample="false"  selectByExampleQueryId="false">
		</table>
		<table tableName="topic_post" domainObjectName="Post" enableCountByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="true" enableUpdateByExample="false"  selectByExampleQueryId="false">
		</table>
		<table tableName="topic_role" domainObjectName="Role" enableCountByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="true" enableUpdateByExample="false"  selectByExampleQueryId="false">
		</table>
		<table tableName="topic_role_permissions" domainObjectName="Permissions" enableCountByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="true" enableUpdateByExample="false"  selectByExampleQueryId="false">
		</table>
		<table tableName="topic_role_permissions_link" domainObjectName="RolePermissionsLink" enableCountByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="true" enableUpdateByExample="false"  selectByExampleQueryId="false">
		</table>
		<table tableName="topic_topic" domainObjectName="Topic" enableCountByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="true" enableUpdateByExample="false"  selectByExampleQueryId="false">
		</table>
		<table tableName="topic_user_role_topic_link" domainObjectName="UserRoleTopicLink" enableCountByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="true" enableUpdateByExample="false"  selectByExampleQueryId="false">
		</table>
		<!--
		-->
	</context>
</generatorConfiguration>
